{
    "output_folder":"data_gen/",
    "tasks":[
        {
            "input_file":"assets/world_land.dat",
            "number_per_proj":10,
            "projections":["merc","eqdc","moll"],
            "color_maps":{
                "palette":["jet","gray","viridis","cubehelix","RdBu","Blues","binary"],
                "positions":["left","right","top","bottom"],
                "quantized":{
                    "nvalues":[10,20],
                    "isrange":true
                },
                "continuous":true
            },
            "background":{
                "ocean_color":["white", "#528db7","#5296b7","#52a0b7","#52a9b7","#52b2b7"],
                "land_colors":"random",
                "background":["bluemarble","shadedrelief","etopo"]
            },
            "basemap":
            {
                "lat_0":"auto",
                "lon_0":"auto",
                "n_tick_lat":6,
                "n_tick_lon":8,
                "llcrnrlat":{
                    "min":-80,
                    "max":-10
                },
                "llcrnrlon":
                {
                    "min":-170,
                    "max":-10
                },
                "urcrnrlat":{
                    "min":10,
                    "max":80
                },
                "urcrnrlon":{
                    "min":10,
                    "max":170
                },
                "lat_ts":20,
                "resolution":"i"
            }
        }
    ]
}